{
  "name": "aurelia2-capacitor-app",
  "description": "An Aurelia 2 and capacitor app",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "???"
  },
  "license": "UNLICENSED",
  "dependencies": {
    "@aurelia/i18n": "^2.0.0-alpha.2",
    "aurelia": "latest",
    "i18next-fetch-backend": "^3.0.0"
  },
  "devDependencies": {
    "@aurelia/testing": "latest",
    "@aurelia/ts-jest": "latest",
    "@aurelia/webpack-loader": "latest",
    "@capacitor/android": "^2.4.7",
    "@capacitor/cli": "^2.4.7",
    "@capacitor/core": "^2.4.7",
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.35",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "copy-webpack-plugin": "^8.1.1",
    "css-loader": "^5.1.3",
    "eslint": "^7.22.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "htmlhint": "^0.14.2",
    "jest": "^26.6.3",
    "jest-transform-stub": "^2.0.0",
    "node-sass": "^7.0.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.18",
    "typescript": "^4.2.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.27.2",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "scripts": {
    "lint:js": "eslint src test --ext .js,.ts",
    "lint:html": "htmlhint -c .htmlhintrc src",
    "lint": "npm run lint:js && npm run lint:html",
    "pretest": "npm run lint",
    "start": "webpack serve",
    "build": "rimraf www && webpack --env production && npx cap copy",
    "analyze": "rimraf www && webpack --env production --analyze",
    "test": "jest",
    "andriod:add": "npx cap add android",
    "andriod:open": "npx cap open andriod",
    "android:sync": "npx cap sync andriod",
    "android:remove": "rm -rf android"
  },
  "jest": {
    "transform": {
      "\\.(css|less|sass|scss|styl|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "jest-transform-stub",
      "\\.(ts|html)$": "@aurelia/ts-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ]
  }
}
